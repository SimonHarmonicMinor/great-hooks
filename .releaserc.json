{
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "publishCmd": "yarn publish --access public --new-version \"${nextRelease.version}\"",
        "successCmd": "node ./setVersion.js ${nextRelease.version}"
      }
    ],
    "@semantic-release/github"
  ]
}
